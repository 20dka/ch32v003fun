{
	"configurations": [
	{
		"name": "GDB debug - custom",
		"type": "cppdbg",
		"request": "launch",
		"program": "blink.elf",
		"args": [],
		"stopAtEntry": true,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
		{
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		}
		],
		"miDebuggerPath": "/usr/bin/gdb-multiarch",
		"miDebuggerServerAddress": "127.0.0.1:2000"
	}
	]
}
